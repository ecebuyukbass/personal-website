datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Article {
  id          Int      @id @default(autoincrement())
  title       String
  slug        String   @unique
  content     String
  year        Int
  category    String   
  status      String
  publishedAt DateTime?
  createdAt DateTime @default(now())

}
